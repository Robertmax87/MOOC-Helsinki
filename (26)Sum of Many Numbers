package com.beg;

import java.util.Scanner;

public class Main {
    public static void main(String[] arg) {
        Scanner reader = new Scanner(System.in);
        int sum = 0;
        while (true) {
            int read = Integer.parseInt(reader.nextLine());
            if (read == 0) {
                break;
            }   sum = read + sum;
                System.out.println("Sum now:" + sum);
            }
        }
    }
